<div id="label-container"></div>
{#<script src="https://docs.opencv.org/4.5.5/opencv.js"></script>#}
<script src="https://cdn.jsdelivr.net/npm/@tensorflow/tfjs/dist/tf.min.js"> </script>
<script src="https://smtpjs.com/v3/smtp.js"></script>


    <div>
        <h1>Detector2 settings</h1>
    </div>
<form class="form-horizontal">
    <div class="control-group">
        <label class="control-label">{{ _('Host') }}</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.detector2.host">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">{{ _('Username') }}</label>
        <div class="controls">
            <input type="email" class="input-block-level" data-bind="value: settings.plugins.detector2.username">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">{{ _('Password') }}</label>
        <div class="controls">
            <input type="password" class="input-block-level" data-bind="value: settings.plugins.detector2.password">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">{{ _('To') }}</label>
        <div class="controls">
            <input type="email" class="input-block-level" data-bind="value: settings.plugins.detector2.to">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">{{ _('Confidence threshold') }}</label>
        <div class="controls">
            <input type="number" class="input-block-level" max="100" min="0" data-bind="value: settings.plugins.detector2.confidence">
        </div>
    </div>

</form>
